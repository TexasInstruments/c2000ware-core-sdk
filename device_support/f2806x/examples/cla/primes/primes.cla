//
// Defines
//
int32 chkdiv(int32, int32);

//
// Cla1Task1 - Generate the sequence of prime numbers
//
__interrupt void
Cla1Task1(void)
{
    __mdebugstop();
    int32 i, j, index;
    index = 0;
   
    //
    // Obviously 1 and 2 are prime, but for this test
    // let the program determine that
    //
    for ( i = 1; i <= 2; i++ )
    {
        for ( j = 2; j <= i/2; j++ )
        {
            if (chkdiv(i,j) == 0)
            {
                break;
            }
        }
        if ( j > i / 2 ) 
        {
            out[index] = i;
            index++;
        }
    }
    
    //
    // For the rest of the numbers, skip evens
    //       
    for ( i = 3; index < LENGTH; i+=2 )
    {
        for ( j = 2; j <= i/2; j++ )
        {
            if (chkdiv(i,j) == 0 )
            {
                break;
            }
        }
        if ( j > i / 2 ) 
        {
            out[index] = i;
            index++;
        }
    }
}

//
// chkdiv -
//
int32
chkdiv(int32 x, int32 y)
{
    x = abs(x);
    y = abs(y);
    do
    {
        x -= y;        
    }
    while (x > 0);
    return x;     
}

