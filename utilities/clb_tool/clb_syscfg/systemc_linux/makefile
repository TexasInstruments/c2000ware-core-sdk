C2000WARE_ROOT:=/home/user/ti/C2000Ware_5_00_00_00/utilities/clb_tool/clb_syscfg
SYSTEMC_INSTALL:=/usr/bin/systemc-2.3.3
BuildOutput:=./src

BuildDirectory:=$(C2000WARE_ROOT)/systemc/src
MODEL_HEADERS_DIR:=$(C2000WARE_ROOT)/systemc/include
MODEL_SOURCE_DIR:=$(C2000WARE_ROOT)/systemc/src
MODEL_HEADERS_DIR_TYPE2:=$(C2000WARE_ROOT)/systemc/include/type2
MODEL_SOURCE_DIR_TYPE2:=$(C2000WARE_ROOT)/systemc/src/type2
MODEL_HEADERS_DIR_TYPE3:=$(C2000WARE_ROOT)/systemc/include/type3
MODEL_SOURCE_DIR_TYPE3:=$(C2000WARE_ROOT)/systemc/src/type3

all:
	g++  -c -DCLB_SIM -I$(SYSTEMC_INSTALL)/src -I$(MODEL_HEADERS_DIR) -Og -g -gdwarf-3 -gstrict-dwarf -Wall -MMD -MP -MF$(BuildOutput)/CLB_LUT4_SC_model.d		-MT$(BuildOutput)/CLB_LUT4_SC_model.o		-I$(BuildDirectory)/syscfg   -fno-threadsafe-statics  -o$(BuildOutput)/CLB_LUT4_SC_model.o 	$(MODEL_SOURCE_DIR)/CLB_LUT4_SC_model.cpp 1> lut4_compile_result0.txt 2>&1
	g++  -c -DCLB_SIM -I$(SYSTEMC_INSTALL)/src -I$(MODEL_HEADERS_DIR) -Og -g -gdwarf-3 -gstrict-dwarf -Wall -MMD -MP -MF$(BuildOutput)/CLB_FSM_SC_model.d		-MT$(BuildOutput)/CLB_FSM_SC_model.o		-I$(BuildDirectory)/syscfg   -fno-threadsafe-statics  -o$(BuildOutput)/CLB_FSM_SC_model.o	$(MODEL_SOURCE_DIR)/CLB_FSM_SC_model.cpp 1> fsm_compile_result0.txt 2>&1
	g++  -c -DCLB_SIM -I$(SYSTEMC_INSTALL)/src -I$(MODEL_HEADERS_DIR) -Og -g -gdwarf-3 -gstrict-dwarf -Wall -MMD -MP -MF$(BuildOutput)/CLB_counter_SC_model.d	-MT$(BuildOutput)/CLB_counter_SC_model.o 	-I$(BuildDirectory)/syscfg   -fno-threadsafe-statics  -o$(BuildOutput)/CLB_counter_SC_model.o	$(MODEL_SOURCE_DIR)/CLB_counter_SC_model.cpp 1> counter_compile_result0.txt 2>&1
	g++  -c -DCLB_SIM -I$(SYSTEMC_INSTALL)/src -I$(MODEL_HEADERS_DIR) -Og -g -gdwarf-3 -gstrict-dwarf -Wall -MMD -MP -MF$(BuildOutput)/CLB_HLC_SC_model.d		-MT$(BuildOutput)/CLB_HLC_SC_model.o		-I$(BuildDirectory)/syscfg   -fno-threadsafe-statics  -o$(BuildOutput)/CLB_HLC_SC_model.o	$(MODEL_SOURCE_DIR)/CLB_HLC_SC_model.cpp 1> hlc_compile_result0.txt 2>&1
	g++  -c -DCLB_SIM -I$(SYSTEMC_INSTALL)/src -I$(MODEL_HEADERS_DIR) -Og -g -gdwarf-3 -gstrict-dwarf -Wall -MMD -MP -MF$(BuildOutput)/CLB_OutputLUT_SC_model.d	-MT$(BuildOutput)/CLB_OutputLUT_SC_model.o	-I$(BuildDirectory)/syscfg   -fno-threadsafe-statics  -o$(BuildOutput)/CLB_OutputLUT_SC_model.o	$(MODEL_SOURCE_DIR)/CLB_OutputLUT_SC_model.cpp 1> outlut_compile_result0.txt 2>&1
