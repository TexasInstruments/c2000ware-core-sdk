	g++  -c -DCLB_SIM -I$(SYSTEMC_INSTALL)/src -I$(MODEL_HEADERS_DIR_TYPE2) -Og -g -gdwarf-3 -gstrict-dwarf -Wall -MMD -MP -MF$(BuildOutput)/type2/CLB_FSM_SC_model.d		-MT$(BuildOutput)/type2/CLB_FSM_SC_model.o		-I$(BuildDirectory)/syscfg   -fno-threadsafe-statics  -o$(BuildOutput)/type2/CLB_FSM_SC_model.o			$(MODEL_SOURCE_DIR_TYPE2)/CLB_FSM_SC_model.cpp 1> fsm_compile_result1.txt 2>&1
